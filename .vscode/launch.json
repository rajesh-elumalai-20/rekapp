{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run rekapp-1",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}/rekapp",
      "runtimeExecutable": "cds",
      "args": [
        "serve",
        "--in-memory?"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/rekapp/.env1",
      "env": {
        "NODE_ENV": "development",
        "run.config": "{\"handlerId\":\"cap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/rekapp\\\",\\\"profileName\\\":\\\"development\\\"}\"}"
      }
    }
  ]
}